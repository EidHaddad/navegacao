{
	"_version": "1.8.0",
	"sap.app": {
		"id": "ovly.navegacao",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "ovly.navegacao.view.S0",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ovly.navegacao.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "ovly.navegacao.view",
				"controlAggregation": "masterPages",
				"controlId": "root",
				"clearControlAggregation": false
			},
			"routes": [
				{
					"name": "padrao",
					"pattern": "",
					"target": [
						"S1",
						"S2"
					]
				},
				{
					"name": "adicionar",
					"pattern": "add",
					"target": [
						"S3", "S1"
					]
				}
			],
			"targets": {
				"S1": {
					"controlAggregation": "detailPages",
					"viewName": "S1",
					"viewlevel": 1						
				},
				"S2": {
					"viewName": "S2",
					"viewlevel": 1					
				},
				"S3": {

					"viewName": "S3",
					"viewlevel": 2					
				}
			}
		}
	}
}